<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC	"-//mybatis.org//DTD Mapper 3.0//EN" 
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kedu.arias.commonMapper">

<select id="selectAllCountry" resultType="com.kedu.arias.common.dto.Country">
	SELECT * FROM COUNTRY_CODE
	WHERE 1=1
	ORDER BY country_name_kor ASC
</select>

</mapper>