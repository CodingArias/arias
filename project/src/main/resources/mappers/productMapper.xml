<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kedu.arias.productMapper">
	<select id="selectAllListByKeyword" resultType="com.kedu.arias.product.dto.ProductDto">
		select * from product where country_id = #{country_id} and city_id = #{city_id}
	</select>
</mapper>

